{
  "name": "PukiWiki Advance",
  "version": "2.1.0-alpha",
  "dependencies": {
    "autosize": "~3.*",
    "bootstrap": "~4.*",
    "fontawesome": "~4.*",
    "jquery": "~2.*",
    "jquery-form": "~3.*",
    "jquery-mobile": "~1.4.*",
    "jquery-ui": "~1.11.*",
    "jquery.cookie": "~1.*",
    "jquery.taboverride": "4.0.0",
    "modernizr": "~3.*",
    "superfish": "~1.*"
  },
  "overrides": {
    "fontawesome": {
      "main": [
        "**/*.min.css",
        "**/Font*",
        "**/*webfont*"
      ]
    },
    "bootstrap": {
      "main": [
        "dist/**/*.min.js",
        "dist/**/*.min.css"
      ]
    },
    "jquery": {
      "main": "**/*.min.js"
    }
  },
  "scripts": {
    "compile": [
      "bower install"
    ]
  }
}
